    gem "jekyll-feed"
    gem "jekyll-seo-tag"
    gem "jekyll-sitemap"
    gem "jekyll-paginate"
    gem "jekyll-algolia"
    gem "jekyll-include-cache"
end
# Para dev local com `bundle exec jekyll serve`
group :development do
